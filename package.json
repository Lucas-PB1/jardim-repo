{
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "vite build && npm run summernote-copy && npm run jquery-copy && npm run toarst-copy && npm run select2-copy && npm run dropzone-copy && npm run font-awesome-copy",
        "dropzone-copy": "npm run copy-dropzone-js && npm run copy-dropzone-css",
        "font-awesome-copy": "npm run copy-font-awesome-js",
        "jquery-copy": "npm run copy-jquery-assets",
        "toarst-copy": "npm run copy-toarst-js && npm run copy-toarst-css",
        "summernote-copy": "npm run copy-summernote-assets-js && npm run copy-summernote-assets-css && npm run copy-summernote-fonts",
        "select2-copy": "npm run copy-select2-js && npm run copy-select2-css && npm run copy-select2-theme",
        "copy-summernote-fonts": "cp -r node_modules/summernote/dist/font/ public/modules/summernote",
        "copy-summernote-assets-js": "cp node_modules/summernote/dist/summernote-lite.min.js public/modules/summernote",
        "copy-summernote-assets-css": "cp node_modules/summernote/dist/summernote-lite.min.css public/modules/summernote",
        "copy-jquery-assets": "cp node_modules/jquery/dist/jquery.min.js public/modules/jquery",
        "copy-toarst-css": "cp node_modules/toastr/build/toastr.css public/modules/toarst",
        "copy-toarst-js": "cp node_modules/toastr/build/toastr.min.js public/modules/toarst",
        "copy-select2-theme": "cp node_modules/select2-bootstrap-5-theme/dist/select2-bootstrap-5-theme.min.css public/modules/select2",
        "copy-select2-css": "cp node_modules/select2/dist/css/select2.min.css public/modules/select2",
        "copy-select2-js": "cp node_modules/select2/dist/js/select2.min.js public/modules/select2",
        "copy-dropzone-css": "cp node_modules/dropzone/dist/dropzone.css public/modules/dropzone",
        "copy-dropzone-js": "cp node_modules/dropzone/dist/dropzone-min.js public/modules/dropzone",
        "copy-font-awesome-js": "cp node_modules/font-awesome/css/font-awesome.min.css public/modules/font-awesome"
    },
    "devDependencies": {
        "@popperjs/core": "^2.11.6",
        "@tailwindcss/forms": "^0.5.2",
        "alpinejs": "^3.4.2",
        "autoprefixer": "^10.4.2",
        "axios": "^1.1.2",
        "bootstrap": "^5.3.1",
        "laravel-vite-plugin": "^0.7.5",
        "postcss": "^8.4.6",
        "sass": "^1.56.1",
        "tailwindcss": "^3.1.0",
        "vite": "^4.0.0"
    },
    "dependencies": {
        "aos": "^2.3.4",
        "dropzone": "^6.0.0-beta.2",
        "font-awesome": "^4.7.0",
        "imask": "^7.1.3",
        "jquery": "^3.7.0",
        "less": "^4.1.3",
        "select2": "^4.1.0-rc.0",
        "select2-bootstrap-5-theme": "^1.3.0",
        "summernote": "^0.8.20",
        "sweetalert2": "^11.10.5",
        "tabulator-tables": "^5.5.1",
        "toastr": "^2.1.4"
    }
}
